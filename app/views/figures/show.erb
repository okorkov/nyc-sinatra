<h2>Name</h2>
<%= @figure.name %>
<br>
<h2>Landmarks</h2>
<ul>
<% @figure.landmarks.each do |landmark| %>
  <li><a href="/landmarks/<%= landmark.id %>"><%= landmark.name %></a></li>
<% end %>
</ul>
<br>
<h2>Titles</h2>
<ul>
<% @figure.titles.each do |title| %>
  <li><a href="/titles/<%= title.id %>"><%= title.name %></a></li>
<% end %>
</ul>
<br>
<a href="/figures/<%= @figure.id %>/edit">Edit Figure</a>
<br>
<a href="/figures">Figures Home</a>